{
  "extends": [
    "@schematics/angular"
  ],
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Creates neo base folder structure.",
      "factory": "./ng-add/index#ngAdd"
    },
    "neo-component": {
      "aliases": ["nc"],
      "description": "Creates a Neo Component",
      "factory": "./component",
      "schema": "./component/schema.json"
    },
    "neo-init": {
      "description": "Initializes project.",
      "factory": "./init",
      "schema": "./init/schema.json"
    },
    "neo-module": {
      "aliases": ["nm"],
      "description": "Creates a Neo Module",
      "factory": "./module",
      "schema": "./module/schema.json"
    },
    "neo-service": {
      "aliases": ["ns"],
      "description": "Creates a Neo Service",
      "factory": "./service",
      "schema": "./service/schema.json"
    }
  }
}